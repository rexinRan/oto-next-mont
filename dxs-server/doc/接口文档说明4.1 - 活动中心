- 游学定制提交
http://192.168.0.10:8080/dxs-server/ActivesCustomController/personalTailor.do?customizedId=5&customCountryId=18&customCoursesId=1&customModeId=1&customDurationId=1&customVisaId=1

    传递的参数:
            /**用户id*/
            userId;
            /**课程id*/
            customizedId;
            /**国家id*/
            customCountryId;
            /**选修课ID*/
            customCoursesId;
            /**住宿方式*/
            customModeId;
            /**课程时长*/
            customDurationId;
            /**签证方式id*/
            customVisaId;
            /**setouttime*/
            setouttime;
            /**紧急联系人*/
            contacts1;
            /**紧急联系电话*/
            phone1;
            /**紧急联系人2*/
            contacts2;
            /**紧急联系电话2*/
            phone2;
            /**是否缴费*/
            result;
            /**创建时间*/
            createTime;
            /**更新时间*/
            updateTime;
            /**删除标志*/
            isDelete;









